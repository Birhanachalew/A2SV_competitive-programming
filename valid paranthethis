class Solution:
    def isValid(self, s: str) -> bool:
        stack = []

        mapp = {"]":"[","}":"{",")":"("}

        for char in s:
            if char in mapp:
                if stack and stack[-1] in mapp[char]:
                    stack.pop()
                else:
                    return False
            else:
                stack.append(char)
        if not stack :
            return True
        return False
