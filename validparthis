class Solution :
    def isValid(self,s : str)-> bool :
        stack = []
        Map = dict
        Map = { ")": "(", "}" : "{", "]" : "[" }
        for c in s :
            if c in Map :
                if stack and stack[-1] == Map[c] :
                    stack.pop()
                else :
                    return False
            else:
                stack.append(c)
        if not stack: 
            return True
        else:
            return False
