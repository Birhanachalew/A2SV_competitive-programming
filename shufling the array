class Solution:
    def shuffle(self, nums: List[int], n: int) -> List[int]:
        new = []

        i = 0

        n = len(nums) // 2
        j = n

        while i < n:

            if True:
                new.append(nums[i])
                i += 1
                new.append(nums[j])

                j += 1
        return new


        
