class Solution:
    def fib(self, n: int) -> int:
        f0=0
        f1=1
        
        if n==1 :
            return n
        if n==0:
            return n
        ans=0
        for i in range(n-1):
            ans=f0+f1
            f0=f1
            f1=ans
        return ans
    
        
        
